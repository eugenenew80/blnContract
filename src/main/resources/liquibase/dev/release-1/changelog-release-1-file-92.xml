<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
		xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

	<changeSet id="cm-rus-1849" logicalFilePath="path-independent" author="ruslan" context="dev">
		<comment>Creating table: cm_pro_cldr_transfer</comment>
		<createTable tableName="cm_pro_cldr_transfer" schemaName="apps" remarks="производственный календарь: праздничные дни">
			<column name="id" type="number" remarks="Идентификатор">
				<constraints nullable="false"/>
			</column>

			<column name="calendar_line_id" type="number" remarks="Идентификатор строки календаря">
				<constraints nullable="false"/>
			</column>

			<column name="transfer_calendar_line_id" type="number" remarks="Идентификатор строки календаря на которую перенесли">
				<constraints nullable="false"/>
			</column>

			<column name="day_type_code" type="varchar2(30)" remarks="код типа дня недели">
				<constraints nullable="true"/>
			</column>

			<column name="first_date" type="date" remarks="Дата которую переносят">
				<constraints nullable="true"/>
			</column>

			<column name="transfer_date" type="date" remarks="Дата на которую перенесли">
				<constraints nullable="true"/>
			</column>

			<column name="is_public_holiday" type="number" remarks="Государственный праздник">
				<constraints nullable="true"/>
			</column>

			<column name="valid_start_date" type="date" remarks="срок действия с">
				<constraints nullable="true"/>
			</column>

			<column name="valid_end_date" type="date" remarks="срок действия по">
				<constraints nullable="true"/>
			</column>

			<column name="create_by" type="number" remarks="Автор">
				<constraints nullable="false"/>
			</column>

			<column name="create_date" type="TIMESTAMP" remarks="Дата создания">
				<constraints nullable="false"/>
			</column>

			<column name="last_update_by" type="number" remarks="Автор обновления">
				<constraints nullable="false"/>
			</column>

			<column name="last_update_date" type="TIMESTAMP" remarks="Дата обновления">
				<constraints nullable="false"/>
			</column>
		</createTable>
	</changeSet>


	<changeSet id="cm-rus-1850" logicalFilePath="path-independent" author="ruslan" context="dev">
		<comment>Creating table: cm_pro_cldr_transfer_tl</comment>
		<createTable tableName="cm_pro_cldr_transfer_tl" schemaName="apps" remarks="Перевод производственный календарь: строки">
			<column name="id" type="number" remarks="Идентификатор">
				<constraints nullable="false"/>
			</column>

			<column name="lang" type="varchar2(10)" remarks="Язык">
				<constraints nullable="false"/>
			</column>

			<column name="transfer_id" type="number" remarks="Ссылка на родительскую запись">
				<constraints nullable="false"/>
			</column>

			<column name="description" type="varchar2(100)" remarks="описание">
				<constraints nullable="true"/>
			</column>

			<column name="create_by" type="number" remarks="Автор">
				<constraints nullable="false"/>
			</column>

			<column name="create_date" type="TIMESTAMP" remarks="Дата создания">
				<constraints nullable="false"/>
			</column>

			<column name="last_update_by" type="number" remarks="Автор обновления">
				<constraints nullable="false"/>
			</column>

			<column name="last_update_date" type="TIMESTAMP" remarks="Дата обновления">
				<constraints nullable="false"/>
			</column>

		</createTable>
	</changeSet>

	<changeSet id="cm-rus-1851" logicalFilePath="path-independent" author="ruslan" context="dev">
		<comment>Creating primary key for table: cm_pro_cldr_transfer_tl</comment>
		<addPrimaryKey
				columnNames="id"
				constraintName="pk_cm_pro_cldr_transfer_tl"
				schemaName="apps"
				tableName="cm_pro_cldr_transfer_tl"/>
	</changeSet>

	<changeSet id="cm-rus-1852" logicalFilePath="path-independent" author="ruslan" context="dev">
		<comment>Creating sequence for table: cm_pro_cldr_transfer_tl</comment>
		<createSequence
				incrementBy="1"
				schemaName="apps"
				sequenceName="cm_pro_cldr_transfer_tl_s"
				startValue="1"/>
	</changeSet>

	<changeSet id="cm-rus-1853" logicalFilePath="path-independent" author="ruslan" context="dev">
		<comment>Creating unique key for table: cm_pro_cldr_transfer_tl</comment>
		<addUniqueConstraint
				columnNames="transfer_id,lang"
				constraintName="uk_cm_pro_cldr_transfer_tl"
				schemaName="apps"
				tableName="cm_pro_cldr_transfer_tl"/>
	</changeSet>

	<changeSet id="cm-rus-1854" logicalFilePath="path-independent" author="ruslan" context="dev">
		<comment>Creating foreign constraint: cm_pro_cldr_transfer_tl to cm_pro_cldr_transfer</comment>
		<addForeignKeyConstraint
				baseTableSchemaName="apps"
				baseTableName="cm_pro_cldr_transfer_tl"
				baseColumnNames="transfer_id"
				constraintName="fk_cm_pro_cldr_transfer_tl_1"
				deferrable="true"
				initiallyDeferred="true"
				onDelete="CASCADE"
				onUpdate="RESTRICT"
				referencedTableSchemaName="apps"
				referencedTableName="cm_pro_cldr_transfer"
				referencedColumnNames="id"
		/>
	</changeSet>

	<changeSet id="cm-rus-1855" logicalFilePath="path-independent" author="ruslan" context="dev">
		<comment>Creating primary key for table: cm_pro_cldr_transfer</comment>
		<addPrimaryKey
				columnNames="id"
				constraintName="pk_cm_pro_cldr_transfer"
				schemaName="apps"
				tableName="cm_pro_cldr_transfer"/>
	</changeSet>

	<changeSet id="cm-rus-1856" logicalFilePath="path-independent" author="ruslan" context="dev">
		<comment>Creating sequence for table: cm_req_con_doc_route_orders</comment>
		<createSequence
				incrementBy="1"
				schemaName="apps"
				sequenceName="cm_pro_cldr_transfer_s"
				startValue="1"/>
	</changeSet>

	<changeSet id="cm-rus-1857" logicalFilePath="path-independent" author="ruslan" context="dev">
		<comment>Creating foreign constraint: cm_pro_cldr_transfer to cm_pro_cldr_lines</comment>
		<addForeignKeyConstraint
				baseTableSchemaName="apps"
				baseTableName="cm_pro_cldr_transfer"
				baseColumnNames="calendar_line_id"
				constraintName="fk_cm_pro_cldr_transfer_1"
				deferrable="true"
				initiallyDeferred="true"
				onDelete="RESTRICT"
				onUpdate="RESTRICT"
				referencedTableSchemaName="apps"
				referencedTableName="cm_pro_cldr_lines"
				referencedColumnNames="id"
		/>
	</changeSet>

	<changeSet id="cm-rus-1858" logicalFilePath="path-independent" author="ruslan" context="dev">
		<comment>Creating foreign constraint: cm_pro_cldr_transfer to cm_pro_cldr_day_types</comment>
		<addForeignKeyConstraint
				baseTableSchemaName="apps"
				baseTableName="cm_pro_cldr_transfer"
				baseColumnNames="day_type_code"
				constraintName="fk_cm_pro_cldr_transfer_1"
				deferrable="true"
				initiallyDeferred="true"
				onDelete="RESTRICT"
				onUpdate="RESTRICT"
				referencedTableSchemaName="apps"
				referencedTableName="cm_pro_cldr_day_types"
				referencedColumnNames="code"
		/>
	</changeSet>
</databaseChangeLog>