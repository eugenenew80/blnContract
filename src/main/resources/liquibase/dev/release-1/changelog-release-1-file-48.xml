				<constraints nullable="true"/>
			</column>

			<column name="bp1_bank_bic" type="VARCHAR2(11)" remarks="Бизнес-партнер 1: БИК банка">
				<constraints nullable="true"/>
			</column>

			<column name="bp1_bank_account_num" type="VARCHAR2(40)" remarks="Бизнес-партнер 1: Номер счета">
				<constraints nullable="true"/>
