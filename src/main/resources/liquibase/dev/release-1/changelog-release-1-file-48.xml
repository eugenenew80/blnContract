				constraintName="pk_cm_rfc_tariff_types_tl"
				schemaName="apps"
				tableName="cm_rfc_tariff_types_tl"/>
	</changeSet>

	<changeSet id="cm-rus-802" logicalFilePath="path-independent" author="ruslan" context="dev">
		<comment>Creating foreign constraint: cm_rfc_tariff_types_tl to cm_rfc_tariff_types</comment>
		<addForeignKeyConstraint
				baseTableSchemaName="apps"
				baseTableName="cm_rfc_tariff_types_tl"
				baseColumnNames="code"
				constraintName="fk_cm_rfc_tariff_types_tl_1"
				deferrable="true"
				initiallyDeferred="true"
				onDelete="CASCADE"
				onUpdate="RESTRICT"
				referencedTableSchemaName="apps"
				referencedTableName="cm_rfc_tariff_types"
				referencedColumnNames="code"
		/>
	</changeSet>

