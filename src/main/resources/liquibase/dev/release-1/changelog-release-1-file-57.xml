	<changeSet id="cm-rus-1103" logicalFilePath="path-independent" author="ruslan" context="dev">
		<comment>Creating table: cm_requests_doc_create_tl</comment>
		<createTable tableName="cm_requests_doc_create_tl" schemaName="apps" remarks="Переводы Запросы на заключение договоров">
			<column name="id" type="number" remarks="Идентификатор">
