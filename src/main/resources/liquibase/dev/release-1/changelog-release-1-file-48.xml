			<column name="bp2_id" type="NUMBER" remarks="Бизнес-партнер 2: Ссылка на бизнес-партнера">
				<constraints nullable="true"/>
			</column>

			<column name="bp2_bin" type="VARCHAR2(12)" remarks="Бизнес-партнер 2: БИН">
				<constraints nullable="true"/>
